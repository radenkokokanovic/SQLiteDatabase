<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="dostavaTheme" >
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/zelenaBoja</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NeuspjelaDostavaTheme" >
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/crvenaBoja</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="crvenaTheme" parent="dostavaTheme">
    <item name="colorPrimary">@color/zelenaBoja</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="NeuspjelaDostavaThema" parent="crvenaTheme">
    <item name="colorPrimary">@color/crvenaBoja</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
    </style>

</resources>
